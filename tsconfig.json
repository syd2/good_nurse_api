{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        // "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "lib": ["es2015"],
    "include": ["src/**/*.ts", "models/**/*.ts", "services/**/*.ts", "utils/**/*.ts"]
}
